{
  "env": {
    "production": {
      "presets": ["es2015", "stage-0"],
      "plugins": [
        ["transform-runtime", {
          "polyfill": false,
          "regenerator": true
        }],
        ["transform-class-properties"],
        ["transform-export-extensions"],
        ["transform-object-rest-spread"],
        ["transform-class-properties"],
        ["transform-es2015-modules-commonjs"],
        ["transform-es2015-destructuring"],
        ["transform-es2015-classes"]
      ]
    },
    "development": {
      "presets": ["es2015", "stage-0"],
      "plugins": [
        ["transform-runtime", {
          "polyfill": false,
          "regenerator": true
        }],
        ["transform-class-properties"],
        ["transform-export-extensions"],
        ["transform-object-rest-spread"],
        ["transform-class-properties"],
        ["transform-es2015-modules-commonjs"],
        ["transform-es2015-destructuring"],
        ["transform-es2015-classes"]
      ]
    },
    "test": {
      "presets": ["es2015", "stage-0"],
      "plugins": [
        ["transform-runtime", {
          "polyfill": false,
          "regenerator": true
        }],
        ["transform-class-properties"],
        ["transform-export-extensions"],
        ["transform-object-rest-spread"],
        ["transform-class-properties"],
        ["transform-es2015-modules-commonjs"],
        ["transform-es2015-destructuring"],
        ["transform-es2015-classes"]
      ]
    }
  }
}
